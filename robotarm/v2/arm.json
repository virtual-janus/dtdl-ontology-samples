[
    {
    "@id": "dtmi:example:robotics:RobotArm;1",
    "@type": "Interface",
    "@context": "dtmi:dtdl:context;3",
    "displayName": "Industrial Robot Arm",
    "description": "An ontology model of an industrial robot arm including physical structure, sensors, and tasks.",
    "contents": [
      {
        "@type": "Component",
        "name": "controller",
        "schema": "dtmi:example:robotics:Controller;1",
        "displayName": "Controller"
      },
      {
        "@type": "Relationship",
        "name": "hasJoints",
        "target": "dtmi:example:robotics:Joint;1",
        "displayName": "Joints",
        "description": "Links to the joints of the robot arm."
      },
      {
        "@type": "Relationship",
        "name": "hasLinks",
        "target": "dtmi:example:robotics:Link;1",
        "displayName": "Links",
        "description": "Links to the rigid links between joints."
      },
      {
        "@type": "Component",
        "name": "endEffector",
        "schema": "dtmi:example:robotics:EndEffector;1",
        "displayName": "End Effector"
      },
      {
        "@type": "Relationship",
        "name": "monitorsWith",
        "target": "dtmi:example:robotics:Sensor;1",
        "displayName": "Sensors",
        "description": "Sensors monitoring robot operation or environment."
      },
      {
        "@type": "Property",
        "name": "workspaceVolume",
        "schema": "double",
        "displayName": "Workspace Volume",
        "description": "Volume of the robot's reachable workspace, in cubic meters."
      },
      {
        "@type": "Telemetry",
        "name": "currentTask",
        "schema": "string",
        "displayName": "Current Task"
      }
    ]
  },
  {
    "@id": "dtmi:example:robotics:Controller;1",
    "@type": "Interface",
    "@context": "dtmi:dtdl:context;3",
    "displayName": "Controller",
    "contents": [
      { "@type": "Property", "name": "controlAlgorithm", "schema": "string" },
      { "@type": "Property", "name": "cycleTime", "schema": "double" },
      { "@type": "Telemetry", "name": "status", "schema": "string" }
    ]
  },
  {
    "@id": "dtmi:example:robotics:EndEffector;1",
    "@type": "Interface",
    "@context": "dtmi:dtdl:context;3",
    "displayName": "End Effector",
    "contents": [
        { "@type": "Property", "name": "toolType", "schema": "string" },
        { "@type": "Property", "name": "gripForce", "schema": "double" },
        { "@type": "Telemetry", "name": "pose", "schema": "string" }
    ]
  },
  {
    "@id": "dtmi:example:robotics:Joint;1",
    "@type": "Interface",
    "@context": "dtmi:dtdl:context;3",
    "displayName": "Joint",
    "contents": [
        { "@type": "Property", "name": "jointType", "schema": "string" },
        { "@type": "Property", "name": "position", "schema": "double" },
        { "@type": "Property", "name": "velocity", "schema": "double" },
        { "@type": "Property", "name": "torque", "schema": "double" },
        { "@type": "Property", "name": "rangeOfMotion", "schema": "double" }
    ]
  },
  {
    "@id": "dtmi:example:robotics:Link;1",
    "@type": "Interface",
    "@context": "dtmi:dtdl:context;3",
    "displayName": "Link",
    "contents": [
      { "@type": "Property", "name": "length", "schema": "double" },
      { "@type": "Property", "name": "mass", "schema": "double" },
      { "@type": "Property", "name": "inertiaTensor", "schema": "string" }
    ]
  },
  {
    "@id": "dtmi:example:robotics:Sensor;1",
    "@type": "Interface",
    "@context": "dtmi:dtdl:context;3",
    "displayName": "Sensor",
    "contents": [
      { "@type": "Property", "name": "sensorType", "schema": "string" },
      { "@type": "Property", "name": "accuracy", "schema": "double" },
      { "@type": "Telemetry", "name": "sensorData", "schema": "string" }
    ]
  }
]
