            "name": "roll",
            "schema": "double",
            "unit": "degrees"
          },
          {
            "@type": "Telemetry",
            "name": "heave",
            "schema": "double",
            "unit": "m"
          },
          {
            "@type": "Telemetry",
            "name": "mooringTension",
            "schema": "double",
            "unit": "kN"
