        "@id": "dtmi:example:FOWT.Nacelle;1",
        "@type": "Interface",
        "displayName": "Nacelle",
        "contents": [
          {
            "@type": "Telemetry",
            "name": "rotationSpeed",
            "schema": "double",
            "unit": "rpm"
          },
          {
            "@type": "Telemetry",
            "name": "temperature",
            "schema": "double",
            "unit": "celsius"
          },
          {
            "@type": "Property",
            "name": "operationalStatus",
            "schema": "string"
          }
        ]
      }
    },
    {
      "@type": "Component",
      "name": "Tower",
      "schema": {
        "@id": "dtmi:example:FOWT.Tower;1",
        "@type": "Interface",
        "displayName": "Tower",
        "contents": [
          {
            "@type": "Telemetry",
            "name": "vibrationLevel",
            "schema": "double",
            "unit": "mm/s"
          },
          {
            "@type": "Telemetry",
            "name": "tiltAngle",
            "schema": "double",
            "unit": "degrees"
          }
        ]
      }
    },
    {
      "@type": "Component",
      "name": "FloatingPlatform",
      "schema": {
