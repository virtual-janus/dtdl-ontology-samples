{
  "@id": "dtmi:example:FOWT;1",
  "@type": "Interface",
  "@context": "dtmi:dtdl:context;2",
  "displayName": "Floating Offshore Wind Turbine",
  "comment": "Self-contained DTDL model for a floating offshore wind turbine, including components, telemetry, properties, commands, and inline relationship targets.",
  "contents": [
    {
      "@type": "Component",
      "name": "Nacelle",
      "schema": {
        "@id": "dtmi:example:FOWT_Nacelle;1",
        "@type": "Interface",
        "displayName": "Nacelle",
        "contents": [
          {
            "@type": "Telemetry",
            "name": "rotationSpeed",
            "schema": "double"
          },
          {
            "@type": "Telemetry",
            "name": "torque",
            "schema": "double"
          },
          {
            "@type": "Telemetry",
            "name": "temperature",
            "schema": "double"
          },
          {
            "@type": "Property",
            "name": "operationalStatus",
            "schema": "string"
          },
          {
            "@type": "Property",
            "name": "generatorHealth",
            "schema": "string"
          }
        ]
      }
    },
    {
      "@type": "Component",
      "name": "Tower",
      "schema": {
        "@id": "dtmi:example:FOWT_Tower;1",
        "@type": "Interface",
        "displayName": "Tower",
        "contents": [
          {
            "@type": "Telemetry",
            "name": "vibrationLevel",
            "schema": "double"
          },
          {
            "@type": "Telemetry",
            "name": "strainGaugeReading",
            "schema": "double"
          },
          {
            "@type": "Property",
            "name": "tiltAngle",
            "schema": "double"
          },
          {
            "@type": "Property",
            "name": "corrosionLevel",
            "schema": "double"
          }
        ]
      }
    },
    {
      "@type": "Component",
      "name": "FloatingPlatform",
      "schema": {
        "@id": "dtmi:example:FOWT_Platform;1",
        "@type": "Interface",
        "displayName": "Floating Platform",
        "contents": [
          {
            "@type": "Telemetry",
            "name": "pitch",
            "schema": "double"
          },
          {
            "@type": "Telemetry",
            "name": "roll",
            "schema": "double"
          },
          {
            "@type": "Telemetry",
            "name": "heave",
            "schema": "double"
          },
          {
            "@type": "Telemetry",
            "name": "acceleration",
            "schema": "double"
          },
          {
            "@type": "Property",
            "name": "anchorIntegrity",
            "schema": "string"
          },
          {
            "@type": "Property",
            "name": "mooringTension",
            "schema": "double"
          }
        ]
      }
    },
    {
      "@type": "Component",
      "name": "Substation",
      "schema": {
        "@id": "dtmi:example:FOWT_Substation;1",
        "@type": "Interface",
        "displayName": "Substation",
        "contents": [
          {
            "@type": "Property",
            "name": "substationID",
            "schema": "string"
          },
          {
            "@type": "Property",
            "name": "connectionVoltage",
            "schema": "double"
          }
        ]
      }
    },
    {
      "@type": "Component",
      "name": "MaintenanceTeam",
      "schema": {
        "@id": "dtmi:example:FOWT_MaintenanceTeam;1",
        "@type": "Interface",
        "displayName": "Maintenance Team",
        "contents": [
          {
            "@type": "Property",
            "name": "teamID",
            "schema": "string"
          },
          {
            "@type": "Property",
            "name": "contactEmail",
            "schema": "string"
          }
        ]
      }
    },
    {
      "@type": "Component",
      "name": "WeatherStation",
      "schema": {
        "@id": "dtmi:example:FOWT_WeatherStation;1",
        "@type": "Interface",
        "displayName": "Weather Station",
        "contents": [
          {
            "@type": "Telemetry",
            "name": "externalWindSpeed",
            "schema": "double"
          },
          {
            "@type": "Telemetry",
            "name": "airPressure",
            "schema": "double"
          },
          {
            "@type": "Telemetry",
            "name": "humidity",
            "schema": "double"
          }
        ]
      }
    },
    {
      "@type": "Property",
      "name": "status",
      "schema": "string",
      "writable": true
    },
    {
      "@type": "Property",
      "name": "location",
      "schema": {
        "@type": "Object",
        "fields": [
          {
            "name": "latitude",
            "schema": "double"
          },
          {
            "name": "longitude",
            "schema": "double"
          }
        ]
      }
    },
    {
      "@type": "Property",
      "name": "gridConnectionStatus",
      "schema": "string"
    },
    {
      "@type": "Telemetry",
      "name": "powerOutput",
      "schema": "double"
    },
    {
      "@type": "Telemetry",
      "name": "windSpeed",
      "schema": "double"
    },
    {
      "@type": "Telemetry",
      "name": "waveHeight",
      "schema": "double"
    },
    {
      "@type": "Telemetry",
      "name": "turbineHealthScore",
      "schema": "double"
    },
    {
      "@type": "Command",
      "name": "shutdown",
      "request": {
        "name": "reason",
        "schema": "string"
      },
      "response": {
        "name": "result",
        "schema": "string"
      }
    },
    {
      "@type": "Relationship",
      "name": "connectedToSubstation",
      "target": "dtmi:example:FOWT_Substation;1"
    },
    {
      "@type": "Relationship",
      "name": "assignedToMaintenanceTeam",
      "target": "dtmi:example:FOWT_MaintenanceTeam;1"
    },
    {
      "@type": "Relationship",
      "name": "monitoredByWeatherStation",
      "target": "dtmi:example:FOWT_WeatherStation;1"
    },
    {
      "@type": "Relationship",
      "name": "mountedOnPlatform",
      "target": "dtmi:example:FOWT_Platform;1"
    }
  ]
}
