[
  {
    "@context": "dtmi:dtdl:context;2",
    "@id": "dtmi:example:windturbine:WindTurbine;1",
    "@type": "Interface",
    "displayName": "Wind Turbine",
    "description": "Wind turbine component with metadata and relationships to subcomponents.",
    "contents": [
      {
        "@type": "Property",
        "name": "manufacturer",
        "schema": "string",
        "writable": true,
        "description": "Manufacturer of the wind turbine."
      },
      {
        "@type": "Property",
        "name": "manufacturedDate",
        "schema": "date",
        "writable": true,
        "description": "Date the turbine was manufactured."
      },
      {
        "@type": "Property",
        "name": "modelNumber",
        "schema": "string",
        "writable": true,
        "description": "Model number of the turbine."
      },
      {
        "@type": "Property",
        "name": "commissioningDate",
        "schema": "date",
        "writable": true,
        "description": "Date the turbine was commissioned."
      },
      {
        "@type": "Property",
        "name": "latitude",
        "schema": "double",
        "writable": true,
        "description": "Latitude coordinate of the turbine location."
      },
      {
        "@type": "Property",
        "name": "longitude",
        "schema": "double",
        "writable": true,
        "description": "Longitude coordinate of the turbine location."
      },
      {
        "@type": "Relationship",
        "name": "containsRotor",
        "target": "dtmi:example:windturbine:Rotor;1",
        "description": "Contains the rotor component"
      },
      {
        "@type": "Relationship",
        "name": "containsGenerator",
        "target": "dtmi:example:windturbine:Generator;1",
        "description": "Contains the generator component"
      },
      {
        "@type": "Relationship",
        "name": "containsBlade1",
        "target": "dtmi:example:windturbine:Blade;1",
        "description": "Contains the first blade component"
      },
      {
        "@type": "Relationship",
        "name": "containsTower",
        "target": "dtmi:example:windturbine:Tower;1",
        "description": "Contains the tower component"
      }
    ]
  },
  {
    "@context": "dtmi:dtdl:context;2",
    "@id": "dtmi:example:windturbine:Rotor;1",
    "@type": "Interface",
    "displayName": "Rotor",
    "contents": [
      {
        "@type": "Property",
        "name": "azimuth",
        "schema": "double",
        "writable": true,
        "description": "Rotor azimuth angle (deg)."
      },
      {
        "@type": "Property",
        "name": "rotSpeed",
        "schema": "double",
        "writable": true,
        "description": "Rotor rotational speed (rev/s)."
      },
      {
        "@type": "Property",
        "name": "rotThrust",
        "schema": "double",
        "writable": true,
        "description": "Total rotor thrust (kN)."
      },
      {
        "@type": "Property",
        "name": "rotTorq",
        "schema": "double",
        "writable": true,
        "description": "Rotor torque (kNm)."
      },
      {
        "@type": "Property",
        "name": "rotPwr",
        "schema": "double",
        "writable": true,
        "description": "Rotor mechanical power (kW)."
      }
    ]
  },
  {
    "@context": "dtmi:dtdl:context;2",
    "@id": "dtmi:example:windturbine:Generator;1",
    "@type": "Interface",
