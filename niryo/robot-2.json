{
  "@id": "dtmi:ukdtc:ned2:Robot;2",
  "@type": "Interface",
  "@context": "dtmi:dtdl:context;2",
  "displayName": "Ned2 Robot",
  "extends": [
    "dtmi:ukdtc:ned2:BaseAsset;1"
