      "schema": "dtmi:ukdtc:ned2:Joint;2"
    },
    {
      "@type": "Component",
      "name": "j4",
      "schema": "dtmi:ukdtc:ned2:Joint;2"
    },
    {
      "@type": "Component",
      "name": "j5",
      "schema": "dtmi:ukdtc:ned2:Joint;2"
    },
